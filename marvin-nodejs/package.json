{
  "name": "marvin",
  "version": "1.0.0",
  "description": "Production-grade Node.js/TypeScript implementation of Marvin - A multi-tool CLI assistant",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "tsx src/app.ts",
    "test": "node --test dist/tests/**/*.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "bin": {
    "marvin": "dist/app.js"
  },
  "keywords": ["cli", "assistant", "ai", "llm", "tools", "automation"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.3",
    "@google/generative-ai": "^0.21.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "duck-duck-scrape": "^2.2.5",
    "fastify": "^5.0.0",
    "glob": "^11.0.0",
    "groq-sdk": "^0.7.0",
    "js-yaml": "^4.1.0",
    "openai": "^4.63.0",
    "ora": "^8.1.0",
    "picocolors": "^1.1.0",
    "rss-parser": "^3.13.0",
    "simple-git": "^3.27.0",
    "sqlite3": "^5.1.7",
    "winston": "^3.14.2",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.5.5",
    "@types/ws": "^8.5.12",
    "prettier": "^3.3.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
